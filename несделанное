#include <iostream>

using namespace std;

int main()
{
    int howmanytimesprayed = 0;
    int yearofbuilding = 1605;
    int yearsofpraying[40];
    int birth = 0;
    int death = 0;
    cout << "Введите два числа- дату рождения и дату смерти через пробел" << endl;
    cin >> birth >> death;
    
    if (birth > 2000 || death > 2000) {
    cout << "Дата рождения или смерти не может быть больше 2000!" << endl;
    return 0;
    }
    
        for (int i = 0; i < 40; i++) {
            if (i == 0) {
            yearsofpraying[i] = yearofbuilding + 10;
            }
            else
         yearsofpraying[i] = yearsofpraying[i - 1] + 10;
         if (birth + 1 < yearsofpraying[i] < death) {
         howmanytimesprayed++;
         }
    }
    cout << howmanytimesprayed; 
    return 0;
}

