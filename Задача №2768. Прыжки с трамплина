#include <iostream>

using namespace std;

int
main ()
{
  bool samemax = false;
  bool samemin = false;
  int max = 0;
  int min = 100000;
  string snumbers[5];
  int numbers[5];
  cout <<
    "Введите пять чисел через пробел" <<
    endl;
  cin >> numbers[0] >> numbers[1] >> numbers[2] >> numbers[3] >> numbers[4];

  for (int i = 0; i < 5; i++)
    {

      if (numbers[i] > max)
	max = numbers[i];

      if (numbers[i] < min)
	min = numbers[i];

    }

  for (int i = 0; i < 5; i++)
    {
      if (numbers[i] == max && samemax == false)
	{
	  cout << "(" << max << ")" << " ";
	  samemax = true;
	}

      else if (numbers[i] == min && samemin == false)
	{
	  cout << "(" << min << ")" << " ";
	  samemin = true;
	}

      else
	cout << numbers[i] << " ";
    }
  int sum =
    numbers[0] + numbers[1] + numbers[2] + numbers[3] + numbers[4] - min -
    max;
  cout << " = " << sum << endl;

  return 0;
}
