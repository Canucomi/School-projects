//c++

#include <iostream>

using namespace std;

int main () {
    float x;
    float y;
    int days = 1;
    
    cin >> x >> y;
    while (x < y) {
        x = x + x / 100 * 10;
        days++;
    }
    cout << days;
    
    return 0;
}

// python 3.8.1

x=int(input())
y=int(input())
day=1
while y-x>0:
    x=x+(x*0.1)
    day+=1
print(day)
